SOLVER = ['gurobi']
PATHS = []
rule all:
    input:
        expand("../steps/{path}.txt", path=PATHS)

rule run_model:
    input: 
        df_path = "../data/{path}.txt",
    params:
        res_path = "../steps/{path}"
    output:
        "../steps/{path}.txt"
    shell:
        "python solv.py {SOLVER[0]} {input.df_path} {params.res_path}"